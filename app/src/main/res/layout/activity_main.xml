            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/order"
                tools:ignore="ContentDescription,DuplicateIds,ImageContrastCheck,MissingConstraints"
                tools:layout_editor_absoluteX="63dp"
                tools:layout_editor_absoluteY="16dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="150dp"
        android:layout_height="85dp"
        android:outlineSpotShadowColor="#4CAF50"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cardView4"
        tools:layout_editor_absoluteY="349dp"
        tools:ignore="MissingConstraints,UnusedAttribute">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:background="@drawable/cardgreen"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yeon_sung"
                android:text="Create New user"
                android:textColor="#0B0B0B"
                tools:ignore="HardcodedText,MissingConstraints"
                tools:layout_editor_absoluteX="30dp"
                tools:layout_editor_absoluteY="67dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_person"
                tools:layout_editor_absoluteX="63dp"
                tools:layout_editor_absoluteY="16dp"
                tools:ignore="ContentDescription,DuplicateIds,MissingConstraints" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="150dp"
        android:layout_height="85dp"
        android:outlineSpotShadowColor="#4CAF50"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toStartOf="@+id/cardView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="349dp"
        tools:ignore="MissingConstraints,UnusedAttribute">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:background="@drawable/cardgreen"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yeon_sung"
                android:text="profile"
                android:textColor="#4CAF50"
                tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck"
                tools:layout_editor_absoluteX="57dp"
                tools:layout_editor_absoluteY="63dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/profile"
                tools:ignore="ContentDescription,DuplicateIds,ImageContrastCheck,MissingConstraints"
                tools:layout_editor_absoluteX="63dp"
                tools:layout_editor_absoluteY="16dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="150dp"
        android:layout_height="85dp"
        android:outlineSpotShadowColor="#4CAF50"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cardView"
        tools:layout_editor_absoluteY="217dp"
        tools:ignore="MissingConstraints,UnusedAttribute">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:background="@drawable/cardgreen"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yeon_sung"
                android:text="All item Menu"
                android:textColor="#4CAF50"
                tools:ignore="HardcodedText,TextContrastCheck"
                tools:layout_editor_absoluteX="47dp"
                tools:layout_editor_absoluteY="63dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/eye_icon"
                tools:ignore="ContentDescription,DuplicateIds,ImageContrastCheck,MissingConstraints"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="16dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
